@model bitproje.Entitys.Tanilar

@{
    ViewData["Title"] = "Delete";
}

<h3>TANI </h3>
<br />


<div>
    <h style="color:red;">BU TANIYI SİLMEK İSTEDİĞİNİZE EMİN MİSİNİZ ?</h>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            TANI TANIMI
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Tanim.Tanitid)  /   @Html.DisplayFor(model => model.Tanim.Taniadi)
        </dd>
    </dl>
    <form asp-action="Delete">
        <a href="/Tanilar/Index/@Model.Mid">LİSTEYE GERİ DÖN</a> &emsp;|&emsp;
        <input type="hidden" asp-for="Taniid" />
        <input type="submit" style="font-size:13px;font-weight:bold;height:27px;width:70px;" value="SİL" class="btn btn-danger" />

    </form>
</div>
